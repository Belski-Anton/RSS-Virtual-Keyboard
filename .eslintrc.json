{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends":[ 
        "eslint-config-airbnb-base",
        "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-var": "error",
        "indent": ["error", 2],
        "no-multi-spaces": "error",
        "space-in-parens": "error",
        "no-multiple-empty-lines": "error",
        "no-use-before-define": "error",
        "no-console": "warn",
        "max-len": ["warn", { "code": 120 }]
    },
    "ignorePatterns": ["*config.js"]
}
